{
    "name": "<%= (extension_name.toLowerCase().replace(/[^\w\s-_]/gi, '').replace(/\s+/g, '-')) %>",
    "version": "0.0.0",
    "license": "<%= extension_license %>",
    "scripts": {
        "clean": "gulp clean",
        "build:test": "gulp build --env=test",
        "build:prod": "gulp build --env=production",
        "build:production": "gulp build --env=production",
        "package:prod": "cross-var gulp build-package --env=production --packageName=$npm_package_name --buildVersion=$npm_config_buildVersion --defaultBuildVersion=$npm_package_version",
        "package:production": "cross-var gulp build-package --env=production --packageName=$npm_package_name --buildVersion=$npm_config_buildVersion --defaultBuildVersion=$npm_package_version",
        "serve": "gulp serve"
    },
    "dependencies": {
        "rxjs": "^5.5.2"
    },
    "devDependencies": {
        "@types/chrome": "0.0.58",
        "babel-core": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "chalk": "^2.3.0",
        "copy-webpack-plugin": "^4.2.0",
        "cross-var": "^1.1.0",
        "css-loader": "^0.28.7",
        "cssimportant-loader": "^0.4.0",
        "del-cli": "^1.1.0",
        "extract-loader": "^1.0.1",
        "extract-text-webpack-plugin": "^2.1.2",
        "file-loader": "^1.1.5",
        "gulp": "github:gulpjs/gulp#4.0",
        "gulp-cli": "^2.0.0",
        "gulp-json-editor": "^2.2.1",
        "gulp-zip": "^4.1.0",
        "html-loader": "^0.5.1",
        "json-post-process-webpack-plugin": "^1.3.0",
        "less": "^2.7.3",
        "less-loader": "^4.0.5",
        "script-loader": "^0.7.2",
        "socket.io": "^2.0.4",
        "socket.io-client": "^2.0.4",
        "style-loader": "^0.19.0",
        "ts-loader": "^3.1.1",
        "tslint": "~5.7.0",
        "typescript": "^2.3.4",
        "typings-for-css-modules-loader": "^1.7.0",
        "url-loader": "^0.6.2",
        "webpack": "^2.5.1",
        "webpack-merge": "^4.1.1",
        "webpack-stream": "^4.0.0"
    }
}